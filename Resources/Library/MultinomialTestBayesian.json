{
	"name": "MultinomialTest",
	"autorun": true,
	"version": "1.00",
	"options": [
    {
      "name": "factor",
      "type": "Variable"
    },
    {
      "name": "counts",
      "type": "Variable"
    },
    {
      "name": "exProbVar",
      "type": "Variable"
    },
    {
      "name": "hypothesis",
      "options": [ "equalProbs", "expectedProbs", "orderRestricts" ],
      "default": "equalProbs",
      "type": "List"
    },
    {
      "name": "countProp",
      "options": [ "descCounts", "descProps" ],
      "default": "descCounts",
      "type": "List"
    },
		{
			"name": "descriptives",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "credibleInterval",
			"type": "Boolean",
			"default": false
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "credibleIntervalInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "bayesDescriptivesPlot",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "bayesDescriptivesPlotcredibleInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"max": 5,
			"min": 0.001,
			"name": "priorConcentration",
			"type": "Number",
			"value": 1
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "summaryPlot",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.000,
			"name": "summaryPlotCredibleInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 450
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 300
		}
  ]
}
